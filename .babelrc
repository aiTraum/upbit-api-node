{
  "plugins": [
    "@babel/plugin-transform-flow-strip-types",
    "@babel/plugin-proposal-class-properties",
    ["@babel/plugin-proposal-object-rest-spread", { "useBuiltIns": true }],
    ["@babel/plugin-transform-runtime", { "corejs": 2 }]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "6.0"
        }
      }
    ],
    "@babel/flow"
  ]
}
